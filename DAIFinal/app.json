{
  "expo": {
    "name": "DAIFinal",
    "slug": "DAIFinal",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Permitir a $(PRODUCT_NAME) acceder a la cámara para escanear códigos QR."
        }
      ],
        [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permitir $(PRODUCT_NAME) usar tu ubicación.",
          "locationAlwaysPermission": "Permitir $(PRODUCT_NAME) usar tu ubicación.",
          "locationWhenInUsePermission": "Permitir $(PRODUCT_NAME) usar tu ubicación."
        }
      ]
    ]
  }
}
