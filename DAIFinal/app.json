        {
          "locationAlwaysAndWhenInUsePermission": "Permitir $(PRODUCT_NAME) usar tu ubicaci칩n.",
          "locationAlwaysPermission": "Permitir $(PRODUCT_NAME) usar tu ubicaci칩n.",
          "locationWhenInUsePermission": "Permitir $(PRODUCT_NAME) usar tu ubicaci칩n."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Permitir $(PRODUCT_NAME) acceder a tu c치mara."
        }
      ]
    ]
  }
}
